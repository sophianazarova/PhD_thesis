@article{Wenne_Klusek_1985,
  title = {Longevity, growth and parasites of {Macoma} balthica {L}. in {Gdansk} {Bay} ({South} {Baltic})},
  volume = {32},
  abstract = {In years 1980 – 1982, samples were collected with a dredge from several sampling stations situated in Gdansk Bay. The growth rate of Macoma balthica was determined by reading of annual growth ring on shell. Moreover, the sex ratio and degree of infection by Trematoda were estimated. In the central part of Gdansk Bay(depth 35 – 75 m), M.balthica lives longest and attains the greatest size of shell. The growth of M.balthica shell is described by von Bertalanffy equation and by logarithmic equation (above 1 year of life). The rate of M.balthic shell growth in 4 years old and older individuals is smaller in Inner Puck Bay than in the remaining part of Gdansk Bay. Likewise, the rate of the increase in dry weight of body and shell is lowest in the Inner Puck Bay. The proportion of females (u to 60\%),being highest in the population from the Puck Bay and from the more shallow part of Gdansk Bay is untypical of M.balthica. The degree of infection of the M.balthica population from Gdansk Bay by Parvatrema affinis (up to 3.7\%) is very low. After a discussionof the literature data reporting maximal length, age and growth rate of M.balthica in various geographic regions, the occurrence of optimal regon characterized by greater growth rate of this bivalve, was postulated. This is partly opposed to the hypothesis of a drop in the M.balthica growth rate with an increase in latitude.},
  number = {1},
  journaltitle = {Polish archives of hydrobiology},
  shortjournal = {Pol. {Arch}. {Hydrobiol}.},
  author = {Wenne, Roman and Klusek, Zygmunt},
  date = {1985},
