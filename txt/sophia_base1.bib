@inproceedings{Babkov_1985,
  title = {О принципах выделения гидрологических сезонов на примере губы {Чупа} {Белого} моря},
  volume = {31 (39)},
  abstract = {В основные гидрологические сезоны (зима и лето) океанологические характеристики изменяются относительно медленно , в переходные сезоны (весна и осень) эти же характеристики изменяются наиболее быстро. Для выделения гидрологических сезонов определяются даты наиболее быстрого измененияя температуры при ее повышении или понижении. Конец весны и начало осени определяются датами, когда скорости изменения температуры достигат максимума; начало весны и конец осени определяются датами перехода воды через ноль. Период отрицательных температур воды соответствует гидрологической зиме. Период между датами максимальных скоростей соответствует гидрололическому лету.},
  booktitle = {Биоценозы губы {Чупы} {Белого} моря и их сезонная динамика. {Исследования} фауны морей.},
