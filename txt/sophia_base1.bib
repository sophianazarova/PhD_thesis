@article{Beukema_et_al_1985,
  title = {Effects of temperature on the length of the annual growing season in the tellinid bivalve {Macoma} balthica ({L}.) living on tidal flats in the {Dutch} {Wadden} {Sea}},
  volume = {90},
  doi = {10.1016/0022-0981(85)90115-7},
  abstract = {Frequent inspection of the shell margins during a 15-yr study of tidal flat populations of Macoma balthica (L.) in the western part of the Wadden Sea revealed that the annual growing season starts earlier (viz. early March compared with late March) if preceding winter temperatures were higher. The reversal of weight gain to weight loss used as an indicator of the end of the growing season occurs earlier in July if early-summer temperatures are higher. Thus, growth is restricted to the range of water temperatures between 4 and 16°C in spring. No growth was observed during the autumn at the same temperatures. Although low and high temperatures appear to delimit the annual spring growing season, total annual growth is not related to the length of the growing season nor to temperatures prevailing during this period. Feeding conditions appear to prevent growth in autumn and determine the highly variable growth rates within the temperature-set growing period in spring.},
  number = {2},
  journaltitle = {Journal of {Experimental} {Marine} {Biology} and {Ecology}},
  shortjournal = {J. {Exp}. {Mar}. {Biol}. {Ecol}.},
  author = {Beukema, J. and Knol, E. and Cadée, G.},
  date = {1985},
  pages = {129--144},
  langid = {english},
  note = {ЗИН: ИП 2969},
  keywords = {сезон роста annual growing season,температура temperature,Dutch Wadden Sea,Macoma balthica}
}

